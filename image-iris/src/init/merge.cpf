# 使用volume创建iris container时，用户创建的数据库不能放在/usr/irissys/mgr下面
# 默认的位置时durable %sys的配置位置，这里是/dur/init/mgr

[Startup]
SystemMode=TEST

[Actions]
CreateResource:Name=%DB_Demo,Description="Demo space resource"
CreateDatabase:Name=Demo,Directory=/usr/irissys/mgr/Demo
CreateNamespace:Name=Demo,Globals=Demo,Interop=1
